<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Olegu" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#835F33</item>
        <item name="colorPrimaryVariant">#835F33</item>
        <item name="colorOnPrimary">@color/black</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#835F33</item>
        <item name="colorSecondaryVariant">#835F33</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="ThemeOverlay.Olegu.FullscreenContainer" parent="">
        <item name="fullscreenBackgroundColor">#835F33</item>
        <item name="fullscreenTextColor">#835F33</item>
    </style>
</resources>